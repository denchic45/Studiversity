CREATE TABLE teacherEventEntity (
event_id TEXT NOT NULL,
teacher_id TEXT NOT NULL,
FOREIGN KEY (event_id) REFERENCES eventEntity(event_id) ON DELETE CASCADE,
FOREIGN KEY (teacher_id) REFERENCES userEntity(user_id) ON DELETE CASCADE,
UNIQUE(event_id, teacher_id)
);

upsert:
INSERT INTO teacherEventEntity VALUES ?
ON CONFLICT(event_id, teacher_id) DO NOTHING;

deleteByEventId:
DELETE FROM teacherEventEntity WHERE event_id =: eventId;